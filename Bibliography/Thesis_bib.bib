   journal = {American Journal of Physics},
   keywords = {P661300665000HYDRODYNAMICSFLUIDSHELMHOLTZ INSTABIL,bubbles,external flows,film flow,flow instability,flow visualisation,liquid films,wakes},
   pages = {394},
   title = {Fluid instabilities and wakes in a soap-film tunnel},
   volume = {67},
   url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1999AmJPh..67..394V&link_type=ABSTRACT%5Cnpapers://faa8f30e-6396-445b-b9d1-3d23a175bfff/Paper/p72},
   year = {1999},
