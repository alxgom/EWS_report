@article{Tran2009,
   abstract = {It is widely thought that in steady, gravity-driven, unobstructed soap-film flows, the velocity increases monotonically downstream. Here we show experimentally that the velocity increases, peaks, drops abruptly, then lessens gradually downstream. We argue theoretically and verify experimentally that the abrupt drop in velocity corresponds to a Marangoni shock, a type of shock related to the elasticity of the film. Marangoni shocks induce locally intense turbulent fluctuations and may help elucidate the mechanisms that produce two-dimensional turbulence away from boundaries.},
   author = {Tuan Tran and Pinaki Chakraborty and Gustavo Gioia and Stanley Steers and Walter Goldburg},
   doi = {10.1103/PhysRevLett.103.104501},
   isbn = {0031-9007},
   issn = {00319007},
   issue = {10},
   journal = {Physical Review Letters},
   pages = {1-12},
   pmid = {19792318},
   title = {Marangoni shocks in unobstructed soap-film flows},
   volume = {103},
   year = {2009},
}
@article{Burgess1999,
   abstract = {We examine the instability of a soap film flow driven by a time-independent force that is spatially periodic in the direction perpendicular to the forcing (Kolmogorov flow). Linear stability analysis of an idealized model of this flow predicts a critical Reynolds number R(c) is approximately equal to the square root of 2. In our soap film experiment, we find a critical value R(c) is approximately equal to 70. This discrepancy can be ascribed to frictional effects from viscous coupling of gas to the film, which is neglected in the idealized model. The kinematic viscosity of the surrounding gas and the thickness of gas layers on each side of the soap film are varied in the experiments to better understand these frictional effects. Our observations indicate that flow in the soap film cannot be decoupled from flow in the surrounding gas.},
   author = {J M Burgess and C Bizon and W D McCormick and J B Swift and H L Swinney},
   issn = {1063-651X},
   issue = {1},
   journal = {Physical review. E, Statistical physics, plasmas, fluids, and related interdisciplinary topics},
   pages = {715-21},
   pmid = {11969812},
   title = {Instability of the Kolmogorov flow in a soap film.},
   volume = {60},
   url = {http://www.ncbi.nlm.nih.gov/pubmed/11969812},
   year = {1999},
}
