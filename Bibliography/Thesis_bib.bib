   issue = {21},
   journal = {Environmental Science & Technology},
   pages = {11693-11701},
   pmid = {23016881},
   title = {Ecosystem Effects of a Tropical Cyclone on a Network of Lakes in Northeastern North America},
   volume = {46},
   url = {http://pubs.acs.org/doi/abs/10.1021/es302063v},
   year = {2012},
}
