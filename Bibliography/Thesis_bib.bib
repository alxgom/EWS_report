@article{Benzi1988,
   abstract = {The authors present a\ndetailed study of the emergence and the long-time behaviour of coherent\nvortices in two-dimensional decaying turbulence. By high-resolution\nnumerical experiments they find that the coherent structures are\nself-similar, i.e. their energy, enstrophy and size satisfy scaling laws.\nMoreover, the knowledge of the statistical distribution of the size of\nthese vortices is sufficient to compute the energy spectrum of the 2D\nturbulent flow and to explain the significant deviations from the\nKraichnan-Batchelor theory. At long times the motion of the fluid is\ndominated by the vortex dynamics, which is strikingly similar to\nthe Hamiltonian motion of few point vortices, as is confirmed by a\ncomparison between the numerical simulations of the two systems. },
   author = {R. Benzi and S. Patarnello and P. Santangelo},
   doi = {10.1088/0305-4470/21/5/018},
   issn = {0951-7715},
   journal = {Energy},
   pages = {1221-1237},
   title = {Self-similar coherent structures in two-dimensional decaying turbulence},
   volume = {21},
   year = {1988},
}
