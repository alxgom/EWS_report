@article{Vorobieff1999,
   abstract = {We present an experimental characterization of cylinder wakes in flowing soap films. From instantaneous velocity and thickness fields, we find the vortex-shedding frequency, mean-flow velocity, and mean-film thickness. Using the empirical relationship between the Reynolds and Strouhal numbers obtained for cylinder wakes in three dimensions, we estimate the effective soap-film viscosity and its dependence on film thickness. We also compare the decay of vorticity with that in a simple Rankine vortex model with a dissipative term to account for air drag.},
   author = {P Vorobieff and R E Ecke},
   issn = {1063-651X},
