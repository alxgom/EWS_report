@article{Krumm,
   abstract = {This document corresponds to the presentation An Introduction To Resource Theories (Example: Nonuniformity Theory) given by Marius Krumm in the mas- ter studies seminar course Selected topics in Mathematical Physics: Quantum Information Theory at the University of Heidelberg. The aim of the talk is to offer an introduction to the young field of resource theories and their application to statistical mechanics and quantum information theory. Such theories take an agent-based approach to physics: One considers an experimenter, who only has limited access to a physical system, and asks in which ways can that experimenter influence that system. Because of the restric- tions, there are resources which have to be consumed to make some operations possible. This presentation introduces resource theories by explaining some of the main results of a particular resource theory, the theory of informational non- equilibrium},
   author = {Marius Krumm},
   title = {An Introduction To Resource Theories ( Example : Nonuniformity Theory )},
}
@article{Linden2009,
   abstract = {The circumstances under which a system reaches thermal equilibrium, and how\nto derive this from basic dynamical laws, has been a major question from the\nvery beginning of thermodynamics and statistical mechanics. Despite\nconsiderable progress, it remains an open problem. Motivated by this issue, we\naddress the more general question of equilibration. We prove, with virtually\nfull generality, that reaching equilibrium is a universal property of quantum\nsystems: Almost any subsystem in interaction with a large enough bath will\nreach an equilibrium state and remain close to it for almost all times. We also\nprove several general results about other aspects of thermalisation besides\nequilibration, for example, that the equilibrium state does not depend on the\ndetailed micro-state of the bath.},
   author = {Noah Linden and Noah Linden and Sandu Popescu and Sandu Popescu and Anthony J. Short and Anthony J. Short and Andreas Winter and Andreas Winter},
   doi = {10.1103/PhysRevE.79.061103},
   issn = {1539-3755},
   journal = {Physical Review E},
   keywords = {Quantum Physics,Statistical Mechanics},
   pages = {061103},
   pmid = {19658469},
   title = {Quantum mechanical evolution towards thermal equilibrium},
   volume = {79},
   url = {http://arxiv.org/abs/0812.2385},
   year = {2009},
}
