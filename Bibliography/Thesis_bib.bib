@article{Ugalde1994,
   abstract = {Symmetry transformations on the input and output code spaces of deterministic finite automata (DFA) are introduced. We show that the symmetry groups of transformations are produced by group DFA (gDFA) whose set of states and set of inputs are subgroups of the symmetric groups Sq and Sk, respectively (q is the number of states and k the number of input symbols). The set of transitions of a gDFA is also a group. The symmetries of the n-moment delay DFA, relevant for cellular automata, are studied in detail. In particular, we show that the n-moment delay DFA on two symbols are self-symmetric. The symmetry gDFA of the 2-moment delay DFA on two symbols is displayed in detail. An algorithm to construct the symmetry gDFA of arbitrary DFA is given. An application of gDFA to cellular automata dynamics is mentioned. ?? 1994.},
   author = {Edgardo Ugalde and Jesús Urías},
   doi = {10.1016/0167-2789(94)90064-7},
   issn = {01672789},
   issue = {1-2},
   journal = {Physica D: Nonlinear Phenomena},
   pages = {178-184},
   title = {Symmetry groups of automata},
   volume = {70},
   year = {1994},
}
@article{Frederickson1983,
   abstract = {Many papers have been published recently on studies of dynamical processes in which the attracting sets appear quite strange. In this paper the question of estimating the dimension of the attractor is addressed. While more general conjectures are made here, particular attention is paid to the idea that if the Jacobian determinant of a map is greater than one and a ball is mapped into itself, then generically, the attractor will have positive two-dimensional measure, and most of this paper is devoted to presenting cases with such Jacobians for which the attractors are proved to have non-empty interior. ?? 1983.},
   author = {Paul Frederickson and James L. Kaplan and Ellen D. Yorke and James A. Yorke},
   doi = {10.1016/0022-0396(83)90011-6},
   issn = {10902732},
   issue = {2},
   journal = {Journal of Differential Equations},
   pages = {185-207},
   pmid = {21607815},
   title = {The liapunov dimension of strange attractors},
   volume = {49},
   year = {1983},
}
